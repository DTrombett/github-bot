{
  "name": "github-bot",
  "version": "0.0.1",
  "description": "A Discord bot that interacts with Github API.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage --noStackTrace --verbose",
    "test:ci": "jest --coverage --noStackTrace --verbose --ci",
    "start": "npm run lint && npm run compile && node .",
    "build": "npm run lint && npm run compile",
    "lint": "eslint src --fix",
    "compile": "tsc",
    "commands": "node ./dist/registerCommands.js",
    "login": "node .",
    "pm2": "pm2 start dist/index.js --max-memory-restart 100M --exp-backoff-restart-delay=1000 --wait-ready --listen-timeout 10000 --shutdown-with-message"
  },
  "author": "DTrombett",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/babel__core": "^7.1.16",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.0",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-jest": "^27.3.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@discordjs/builders": "^0.7.0",
    "@discordjs/collection": "^0.2.1",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@sapphire/async-queue": "^1.1.7",
    "bufferutil": "^4.0.5",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.2.0",
    "dotenv": "^10.0.0",
    "node-fetch": "^2.6.1",
    "superstruct": "^0.15.3",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.7",
    "winston": "^3.3.3",
    "zlib-sync": "^0.1.7"
  }
}
