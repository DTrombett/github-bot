{
  "name": "github-bot",
  "version": "0.0.1",
  "description": "A Discord bot that interacts with Github API.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage --noStackTrace --verbose",
    "test:ci": "jest --coverage --noStackTrace --verbose --ci",
    "start": "npm run lint && npm run compile && node .",
    "build": "npm run lint && npm run compile",
    "lint": "eslint src --fix",
    "compile": "tsc",
    "commands": "node ./dist/registerCommands.js",
    "login": "node .",
    "pm2": "pm2 start dist/index.js --max-memory-restart 100M --exp-backoff-restart-delay=1000 --wait-ready --listen-timeout 10000 --shutdown-with-message"
  },
  "author": "DTrombett",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/babel__core": "^7.1.16",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.0",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-jest": "^27.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.3",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/collection": "^0.2.1",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@sapphire/async-queue": "^1.1.4",
    "bufferutil": "^4.0.4",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "node-fetch": "^2.6.1",
    "superstruct": "^0.15.2",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.6",
    "winston": "^3.3.3",
    "zlib-sync": "^0.1.7"
  }
}
