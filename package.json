{
  "name": "github-bot",
  "version": "0.0.1",
  "description": "A Discord bot that interacts with Github API.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage --noStackTrace --verbose",
    "test:ci": "jest --coverage --noStackTrace --verbose --ci",
    "start": "npm run lint && npm run compile && node .",
    "build": "npm run lint && npm run compile",
    "lint": "eslint src --fix",
    "compile": "tsc",
    "commands": "node ./dist/registerCommands.js",
    "login": "node .",
    "pm2": "pm2 start dist/index.js --max-memory-restart 100M --exp-backoff-restart-delay=1000 --wait-ready --listen-timeout 10000 --shutdown-with-message"
  },
  "author": "DTrombett",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@tsconfig/node16": "^1.0.3",
    "@types/babel__core": "^7.1.19",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.14.2",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-jest": "^28.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/collection": "^0.7.0",
    "@discordjs/rest": "^0.5.0",
    "@sapphire/async-queue": "^1.3.1",
    "bufferutil": "^4.0.6",
    "discord-api-types": "^0.34.0",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "node-fetch": "^2.6.1",
    "superstruct": "^0.16.0",
    "tslib": "^2.4.0",
    "utf-8-validate": "^5.0.9",
    "winston": "^3.7.2",
    "zlib-sync": "^0.1.7"
  }
}
